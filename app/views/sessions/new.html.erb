<div class="row">
<div class="col-xs-4 col-xs-offset-4">
<h1>ログイン</h1>

<%= form_for :session, url: login_path do |f| %>
  <p>
  番号
  <br>
   <%= f.text_field :banngou,{class: "btn btn-default btn-sm btn-block"} %>
  </p>

  <p>
  パスワード
  <br>
    <%= f.password_field :password,{class: "btn btn-default btn-sm btn-block"} %>
  </p>

  <p>
    <%= f.submit 'ログイン' %>
  </p>
<% end %>
</div>
</div>
